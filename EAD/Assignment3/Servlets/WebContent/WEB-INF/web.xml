<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4"
    xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <display-name>hellowworldservlet</display-name>

    <filter>
   	 <filter-name>helloWorld</filter-name>
   	 <filter-class>com.Filters.HelloWorldFilter</filter-class>
    </filter>
    <filter-mapping>
   	 <filter-name>helloWorld</filter-name>
   	 <url-pattern>/HelloServlet</url-pattern>
    </filter-mapping>

    <servlet>
   	 <display-name>myservlet</display-name>
   	 <servlet-name>HelloServlet</servlet-name>
   	 <servlet-class>com.servlets.HelloServlet</servlet-class>
    </servlet>
    <servlet-mapping>
   	 <servlet-name>HelloServlet</servlet-name>
   	 <url-pattern>/HelloServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
   	 <display-name>AddEmployeeServlet</display-name>
   	 <servlet-name>AddEmployee</servlet-name>
   	 <servlet-class>com.servlets.AddEmployee</servlet-class>
    </servlet>
    <servlet-mapping>
   	 <servlet-name>AddEmployee</servlet-name>
   	 <url-pattern>/AddEmployee</url-pattern>
    </servlet-mapping>
    
    <servlet>
   	 <display-name>ShowAllEmployeeServlet</display-name>
   	 <servlet-name>ShowAllEmployee</servlet-name>
   	 <servlet-class>com.servlets.ShowAllEmployee</servlet-class>
    </servlet>
    <servlet-mapping>
   	 <servlet-name>ShowAllEmployee</servlet-name>
   	 <url-pattern>/ShowAllEmployee</url-pattern>
    </servlet-mapping>
    
    <welcome-file-list>
   	 <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
