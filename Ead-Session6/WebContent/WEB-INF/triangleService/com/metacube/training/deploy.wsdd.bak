<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from triangleService WSDL service -->

  <service name="triangle" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://training.metacube.com"/>
      <parameter name="wsdlServiceElement" value="triangleService"/>
      <parameter name="schemaQualified" value="http://training.metacube.com"/>
      <parameter name="wsdlServicePort" value="triangle"/>
      <parameter name="className" value="com.metacube.training.TriangleSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="triangle"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="area" qname="operNS:area" xmlns:operNS="http://training.metacube.com" returnQName="retNS:areaReturn" xmlns:retNS="http://training.metacube.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:height" xmlns:pns="http://training.metacube.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:width" xmlns:pns="http://training.metacube.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="area"/>

  </service>
</deployment>
